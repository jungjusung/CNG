<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.gnc">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".defaultAct">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".StartActivity"/>
        <activity android:name=".SettingActivity"/>
        <activity android:name=".com.example.user.gnc.settings.IconSettingActivity"/>
        <activity android:name=".com.example.user.gnc.settings.KeySettingActivity"/>
        <activity android:name=".com.example.user.gnc.settings.LocationSettingActivity"/>
        <activity android:name=".com.example.user.gnc.settings.SizeSettingActivity"/>
        <activity android:name=".com.example.user.gnc.settings.AppListActivity"/>
    </application>

</manifest>